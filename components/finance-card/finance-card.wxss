.finance-card {
  background-color: #eee;
  padding: 0 20rpx;
}

.finance-card-content {
  position: relative;
  border-radius: 20rpx;
  overflow: hidden;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}

.finance-card-title {
  display: flex;
  align-items: center;
  padding: 20rpx;
}

.finance-card-title image {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
}

.finance-card-name {
  display: inline-block;
  font-weight: 400;
  margin: 20rpx;
  font-size: 34rpx;
  color: #3d4145;
}

.finance-card-money-des {
  font-size: 26rpx;
  text-align: center;
  margin: 10rpx 0;
  color: #a3a3a3;
}

.finance-card-money {
  text-align: center;
  font-size: 60rpx;
  color: #404040;
  line-height: 1;
}

.finance_card_body {
  padding-bottom: 40rpx;
  display: flex;
  align-items: flex-end
}

.finance_card_content {
  width: 50%;
  display: inline-block;
}

.finance-card-button {
  margin: 0 40rpx;
  border-radius: 70px;
  width: 242rpx;
  height: 70rpx;
  color: #fff;
  line-height: 70rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #ff5700;
}

.recommend {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}